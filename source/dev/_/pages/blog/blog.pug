extends ../../templates/default
block vars
	-
		title = "Блог"
		breadcrumbs = [
			['Главная', 'index.html'],
			[title]
		]
block content
	.page-head
		.case
			.page__wrap
				+breadcrumbs
				.page-head__title.h1 Блог
				.page-head__logo
					img(src="_/uploads/logo-black.png " alt="")

	.blog
